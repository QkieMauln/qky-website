{
  "name": "vue3-vite-template",
  "version": "0.0.1",
  "private": false,
  "description": "A Vite2.x + Vue3 + TypeScript template starter",
  "scripts": {
    "dev": "vite",
    "ssr-dev": "ts-node server",
    "build": "vite build",
    "vbuild": "vue-cli-service build",
    "build-tsc": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "format": "prettier --write ./src",
    "serve-build": "serve -s dist -l 3000",
    "lint": "eslint ./src --ext .vue,.js,.ts",
    "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts"
  },
  "dependencies": {
    "@hcaptcha/vue3-hcaptcha": "^1.0.1",
    "@types/twemoji": "^12.1.2",
    "@types/twemoji-parser": "^13.1.1",
    "animate.css": "^4.1.1",
    "axios": "^0.21.1",
    "dayjs": "^1.10.7",
    "express": "^4.17.3",
    "marked": "^4.0.12",
    "pinia": "^2.0.0-rc.9",
    "prismjs": "^1.27.0",
    "twemoji": "^13.1.0",
    "twemoji-parser": "^13.1.0",
    "vue": "^3.2.6",
    "vue-family-tree": "^0.1.8",
    "vue-prism-editor": "^2.0.0-alpha.2",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.26",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "@vitejs/plugin-vue": "^1.6.0",
    "@vue/compiler-sfc": "^3.2.6",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.8.0",
    "prettier": "^2.2.1",
    "sass": "^1.39.2",
    "typescript": "^4.1.3",
    "vite": "^2.8.6",
    "vue-tsc": "^0.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript/recommended",
      "@vue/prettier",
      "@vue/prettier/@typescript-eslint"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
